{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "Data",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "entity": {
      "type": "string",
      "description": "Add entity to the domain (singular)",
      "alias": "t"
    },
    "domain": {
      "type": "string",
      "description": "The name of the domain library.",
      "$default": {
        "$source": "projectName"
      }
    },
    "inmemory": {
      "type": "boolean",
      "description": "Would you like to add a inmemory data access?",
      "default": false
    },
    "injectable": {
      "type": "boolean",
      "description": "Would you like to use dependency injection?",
      "default": true
    },
    "tags": {
      "type": "string",
      "description": "Add tags to the project (used for linting)",
      "alias": "t"
    },
    "directory": {
      "type": "string",
      "description": "A directory where the project is placed",
      "alias": "d"
    }
  },
  "required": ["name", "entity", "domain"]
}
